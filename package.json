{
  "name": "browserify-angular-injector",
  "version": "1.0.2",
  "description": "This browserify plugin is meant to solve an annoying problem of minification and dependency injection in angular.js.",
  "keywords": [
    "browserify",
    "angular",
    "angularjs",
    "minification",
    "di"
  ],
  "homepage": "https://github.com/alexgorbatchev/browserify-angular-injector",
  "bugs": "https://github.com/alexgorbatchev/browserify-angular-injector/issues",
  "author": {
    "name": "Alex Gorbatchev",
    "url": "https://github.com/alexgorbatchev"
  },
  "main": "./lib/browserify-angular-injector.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexgorbatchev/browserify-angular-injector.git"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha test/*.spec.coffee",
    "pretest": "./node_modules/.bin/coffee --bare --output ./lib --compile ./src/*.coffee"
  },
  "dependencies": {
    "browserify-through": "^2.0.0",
    "angular-injector": "^1.0.0"
  },
  "devDependencies": {
    "coffee-script": "^1.7.1",
